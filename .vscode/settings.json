{
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "launch": {
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Node: Nodemon",
        "cwd": "${workspaceFolder}/back",
        "processId": "${command:PickProcess}",
        "restart": true,
        "protocol": "inspector",
        "skipFiles": ["${workspaceFolder}/node_modules/**/*.js"]
        // "port": 3065
      }
    ]
  }
}
